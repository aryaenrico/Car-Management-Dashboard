<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Car Management Dashboard</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
     <link rel="stylesheet" href="style.css"> 
</head>
  <body>
    <header>
      <div class="container-fluid d-flex flex-row justify-content-between" style="border: 3px solid yellow;">
            <h1 class="title">List Car</h1>
            <button class="btn btn-primary add-new-car" type="button" id="add-car"><a href="/car">+ Add New Car</a></button >
      </div>
    </header>
      <section class="category">
            <div class="container-fluid ">
                  <button class="btn btn-category button-category" type="button">All</button>
                  <button class="btn btn-category btn-margin" type="button">Small</button>
                  <button class="btn btn-category btn-margin" type="button">Medium</button>
                  <button class="btn btn-category btn-margin" type="button">Large</button>
            </div>

      </section>

      <section class="data-car">
            <div class="container-fluid car-list">
                  <div class="row">
                        <div class="col-4">
                              <div class="card card-container" style="width: 22rem;">
                                    <div class="container-img d-flex justify-content-center">
                                          <img class="card-img-top img-status" src="image_1.png" alt="Logo Mobil">
                                    </div>
                                    
                                    <div class="card-body container-text-body">
                                      <h5 class="card-title">Nama/Tipe Mobil</h5>
                                      <p class="card-text harga">Rp 430.000 / hari</p>
                                      <img src="../img/fi_clock.png" alt="logo-waktu">
                                      <p class="card-text updated-text">Updated at 4 Apr 2022, 09.00</p>
                                    </div>

                                    <div class="container-fluid button-area ">
                                          <div class="row justify-content-around">
                                                <div class="col-5">
                                                      <button type="button" class="btn btn-outline-danger btn-delete-car"><img src="Vector_1.png" alt="hapus-logo"><a href="" class="delete-color">DELETE</a></button>
                                                </div>


                                                <div class="col-5">
                                                      <button type="button" class="btn btn-success btn-delete-car"><img src="fi_edit.png" alt="edit-logo"><a href="" class="edit-color">EDIT</a></button>
                                                </div>
                                                     
                                          </div>
                                        

                                    </div>
                              </div>
                        </div>

                        <div class="col-4">
